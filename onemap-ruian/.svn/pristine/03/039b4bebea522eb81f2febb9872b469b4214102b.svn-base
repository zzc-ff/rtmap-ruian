     */
    @Test
    public void weather(){
        MallVo mallVo = new MallVo();
        mallVo.setStartTime("20180101");
        mallVo.setEndTime("20200101");
        R r = appMallAnalysisController.weather(mallVo);
        logger.info("r ==>> {}",JSON.toJSONString(r));
    }

    /**
