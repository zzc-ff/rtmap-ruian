package com.rtmap.modules.app.entity;

import lombok.Data;

@Data
public class ShopEntity {
    /**
     * 店铺Id
     */
    private String shopId;
    /**
     * 店铺名称
     */
    private String shopName;
    /**
     * 销售额
     */
    private long saleAmount;
    /**
     * 日均销售坪效
     */
    private long  saleEffect;
    /**
     * mall 下销售额排名
     */
    private int  saleSort;
    /**
     * mall 的楼层下 销售额排名
     */
    private int floorSort;
    /**
     * mall 的业态下 销售额排名
     */
    private int industrySort;
    /**
     * mall 下店铺总数
     */
    private int mallShopNum;
    /**
     * 业态下店铺总数
     */
    private int industryShopNum;
    /**
     * 楼层下店铺总数
     */
    private int floorShopNum;

}
