    /**
     * 本年排名
     */
    @Test
    public void sort(){
        MallVo mallVo = new MallVo();
        mallVo.setGroupId("集团-1");
        mallVo.setMarketId("项目-11");
        System.out.println("---");
        mallVo.setDateType("Y");
        mallVo.setStartTime("20190101");

